<div class="row">
  <h2>Greeted Names</h2>
  <table>
    <thead>
      <tr>
        <th>names</th>
        <th>languages</th>
        <th>count</th>
      </tr>
    </thead>
    <tbody>
      {{#each nameKept}}
      <tr>
        <td>{{name}}</td>
        <td>{{language}}</td>
        <td>{{count}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>

</div>
